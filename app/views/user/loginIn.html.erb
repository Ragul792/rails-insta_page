<h1>Login Page</h1>
<%= form_with url: root_path ,method: :get do |f| %>
    <p>
        <%= f.label 'email:'%><br>
        <%= f.text_field :email, id: 'email' %>
    </p>
    <p>
        <%= f.label 'password:'%><br>
        <%= f.password_field :password, id: 'password' %>
    </p>
    <p>
        <%= f.submit 'Log In' %>
        <%= f.submit 'Back' %>
    </p>
<%end%>