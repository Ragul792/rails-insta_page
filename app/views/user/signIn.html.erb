<h1>Sign In</h1>
<%= form_with url: root_path ,method: :get do |f| %>
    <p>
        <%= f.label 'Name:'%><br>
        <%= f.text_field :email, id: 'Name' %>
    </p>
    <p>
        <%= f.label 'email:'%><br>
        <%= f.text_field :email, id: 'email' %>
    </p>
    <p>
        <%= f.label 'password:'%><br>
        <%= f.password_field :password, id: 'password' %>
    </p>
    <p>
        <%= f.label 'Role:'%><br>
        <%= f.text_field :email, id: 'Role' %>
    </p>
    <p>
        <%= f.submit 'Sign In' %>
        <%= f.submit 'Back' %>
    </p>
<% end %>